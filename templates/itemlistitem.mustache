<tr id="mod_poodlltime_item_row_{{id}}" class="mod_poodlltime_item_row" data-id="{{id}}">
    <td class="itemid cell c0" style="">{{name}}</td>
    <td class="itemname cell c1" style="">{{typelabel}}</td>
    <td class="itemdate cell c2" style="">
        <a href="#" class="mod_poodlltime_item_move" data-id="{{id}}" data-direction="up">{{#pix}}{{up.key}},{{up.component}},{{up.title}}{{/pix}}</a>
        <a href="#" class="mod_poodlltime_item_move" data-id="{{id}}" data-direction="down">{{#pix}}{{down.key}},{{down.component}},{{down.title}}{{/pix}}</a>
    </td>
    <td class="itemedit cell c3" style=""><a href="#" class="mod_poodlltime_item_row_editlink" data-id="{{id}}" data-qtype='{{type}}' data-type="edit">{{#str}}edititem, mod_poodlltime{{/str}}</a></td>
    <td class="delete cell c4 lastcol" style=""><a href="#" class="mod_poodlltime_item_row_deletelink" data-id="{{id}}" data-name="{{name}}" data-type="delete">{{#str}}deleteitem, mod_poodlltime{{/str}}</a></td>
</tr>